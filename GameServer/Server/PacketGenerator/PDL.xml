<?xml version="1.0" encoding="utf-8" ?>
<PDL>
  <packet name ="C_PlayerInfoReq">
    <byte name ="testByte"/>
    <long name="playerID"/>
    <string name ="name"/>
    <list name ="skill">
      <int name="id"/>
      <short name="level"/>
      <float name="duration"/>
      <list name ="attribute">
        <int name ="att"/>
      </list>
    </list>
  </packet>
  <packet name="S_Test">
    <int name ="testInt"/>
  </packet>

  <packet name ="C_Chat">
    <string name ="chat"/>
  </packet>
  <packet name ="S_Chat">
    <int name ="playerId"/>
    <string name ="chat"/>
  </packet>

  <!-- float 대신에 Vector로 파싱 해서 사용하는 방법도 있음 -->
  <packet name ="S_BroadcastEnterGame">
    <int name="playerId"/>
    <float name="posX"/>
    <float name="posY"/>
    <float name="posZ"/>
  </packet>
  <packet name="C_LeaveGame">
    
  </packet>
  <packet name="S_BroadcastLeaveGame">
    <int name="playerId"/>
  </packet>
  <packet name="S_PlayerList">
    <list name="player">
      <bool name="isSelf"/>
      <int name="playerId"/>
      <float name="posX"/>
      <float name="posY"/>
      <float name="posZ"/>
    </list>
  </packet>
  <packet name="C_Move">
    <float name="posX"/>
    <float name="posY"/>
    <float name="posZ"/>
  </packet>
  <packet name="S_BroadcastMove">
    <int name="playerId"/>
    <float name="posX"/>
    <float name="posY"/>
    <float name="posZ"/>
  </packet>
</PDL>